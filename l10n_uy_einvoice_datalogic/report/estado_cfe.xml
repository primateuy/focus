<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="estado_cfe_emitido">
        <t t-set="company" t-value="docs.company_id" />
        <XMLEntrada>
            <Datos>
                <Dato>
                    <Valor>
                        <startcdata/>
                        <XMLParametros xmlns="GFE_Client">
                            <Empresa t-out="company.company_code"></Empresa>
                            <RangoEnviados>
                                <DocCodERPInicial t-out="docs.id"></DocCodERPInicial>
                                <DocCodERPFinal t-out="docs.id"></DocCodERPFinal>
                            </RangoEnviados>
                        </XMLParametros>
                        <endcdata/>
                    </Valor>
                </Dato>
            </Datos>
        </XMLEntrada>

    </template>

</odoo>
