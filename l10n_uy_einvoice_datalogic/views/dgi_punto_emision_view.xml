<odoo>
    <data>

        <record id="view_dgi_punto_emision_tree_extension_uruware" model="ir.ui.view">
            <field name="name">dgi.punto.emision.tree.extension.uruware</field>
            <field name="model">dgi.punto.emision</field>
            <field name="inherit_id" ref="l10n_uy_einvoice_base.view_dgi_punto_emision_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='dgi_sucursal_id']" position="after">
                    <field name="emision_code"/>
                </xpath>
            </field>
        </record>

        <record id="view_dgi_punto_emision_form_extension_uruware" model="ir.ui.view">
            <field name="name">dgi.punto.emision.form.extension.uruware</field>
            <field name="model">dgi.punto.emision</field>
            <field name="inherit_id" ref="l10n_uy_einvoice_base.view_dgi_punto_emision_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='dgi_sucursal_id']" position="after">
                    <field name="emision_code"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
